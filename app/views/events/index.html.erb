<div class="container main-body" >
  <div class="row main-row">
    <div class="col-md-9 " >
		<%= render "events/header" %>
    	<div class="row view-column" >
	    <h2 id="month">
	      <%= link_to "<", :date => (@date.beginning_of_month-1) %>
	      <%=h @date.strftime("%B %Y") %>
	      <%= link_to ">", :date => (@date.end_of_month+1) %>
	    </h2>
		<%= render "calendar/calendar" %>
		<%= render "map/map" %>
 		</div>
 	</div>
    <div class="col-md-3 about-column">
		<%= render "events/details" %>
	</div>
 </div>
</div>